{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build && cd build && echo '/2.0.0/*  /:splat' > _redirects",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,css,scss,js,mdx}\"",
    "prepare": "husky install",
    "cspell": "cspell  \"**/*.{ts,tsx,css,scss,js,mdx}\""
  },
  "dependencies": {
    "@docusaurus/core": "^2.1.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@docusaurus/theme-mermaid": "^2.3.1",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "clsx": "1.2.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "file-loader": "^6.2.0",
    "plugin-image-zoom": "github:flexanalytics/plugin-image-zoom",
    "postcss": "^8.4.20",
    "postcss-preset-env": "^7.8.3",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.4",
    "react-player": "^2.11.0",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "1.54.8",
    "tailwind": "^4.0.0",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "got": "^12.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-spellcheck": "^0.0.20",
    "husky": "^8.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "*"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx, jsx, ts, mdx}": [
      "eslint"
    ],
    "**/*.{js,ts,tsx, jsx, ts, mdx, css, scss}": [
      "prettier"
    ],
    "**/*.mdx": [
      "cspell"
    ]
  }
}
